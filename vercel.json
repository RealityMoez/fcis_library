{
    "version": 2,
    "builds": [
        {
            "src": "main.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/public/(.*)",
            "dest": "/public/$1"
        }, 
        {
            "src": "/(.*)",
            "dest": "/main.html"
        }
    ]
}
